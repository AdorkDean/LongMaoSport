    <RelativeLayout
        android:id="@+id/coin_rl_pay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/coin_line"
        android:background="@color/back"
        android:paddingTop="10dp">

        <RelativeLayout
            android:id="@+id/coin_rl_c"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/account_f"
            android:padding="@dimen/account_f">

            <RadioGroup
                android:id="@+id/rg_select"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_centerInParent="true"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <RadioButton
                    android:id="@+id/rbtn_zfbpay"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/account_h"
                    android:button="@drawable/radiobutton"
                    android:checked="true"
                    android:paddingLeft="@dimen/account_i"
                    android:text="支付宝支付" />

                <RadioButton
                    android:id="@+id/rbtn_wxpay"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/account_j"
                    android:layout_marginRight="@dimen/account_h"
                    android:button="@drawable/radiobutton"
                    android:visibility="gone"
                    android:paddingLeft="@dimen/account_i"
                    android:text="微信支付" />
            </RadioGroup>
        </RelativeLayout>

        <Button
            android:id="@+id/coin_pay"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@+id/coin_rl_c"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/btn_select_bg"
            android:text="充值"
            android:textColor="@color/white"
            android:textSize="16sp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:padding="30dp">

            <TextView
                android:id="@+id/phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="如遇充值问题，点此联系客服"
                android:textSize="14sp" />
        </RelativeLayout>
